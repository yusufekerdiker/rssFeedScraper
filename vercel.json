{
    "builds": [
        {
            "src": "/Backend/*.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
      {
        "dest": "/Backend/api.py",
        "src": "/api/(.*)"
      },
      {
        "dest": "/Backend/scraper.py",
        "src": "/api/"
      }
    ]
  }
  